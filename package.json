      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "http-errors": "~1.6.3",
    "http-proxy": "^1.18.1",
    "jquery": "^3.5.0",
    "jquery.json-viewer": "^1.4.0",
    "morgan": "~1.9.1",
    "node-rest-client": "^3.1.0",
    "passport": "^0.4.1",
    "passport-adobe-oauth2": "^1.0.1",
    "popper.js": "^1.16.1",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "url-join": "^4.0.1"
  }
