{
  "name": "adobe-io-playground",
  "version": "0.0.0",
  "private": true,
  "description": "Adobe I/O Playground",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon",
    "lint": "eslint . --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
